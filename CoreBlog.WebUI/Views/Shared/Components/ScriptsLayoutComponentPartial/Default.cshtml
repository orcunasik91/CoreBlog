<script src="~/CoreBlogTheme/js/jquery-2.2.3.min.js"></script>
<script src="~/CoreBlogTheme/js/move-top.js"></script>
<script src="~/CoreBlogTheme/js/easing.js"></script>
<script>
    jQuery(document).ready(function ($) {
        $(".scroll").click(function (event) {
            event.preventDefault();
            $('html,body').animate({
                scrollTop: $(this.hash).offset().top
            }, 900);
        });
    });
</script>
<script>
    $(document).ready(function () {
        /*
            var defaults = {
            containerID: 'toTop', // fading element id
            containerHoverID: 'toTopHover', // fading element hover id
            scrollSpeed: 1200,
            easingType: 'linear'
                };
        */
        $().UItoTop({
            easingType: 'easeOutQuart'
        });
    });
</script>
