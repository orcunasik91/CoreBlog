@{ Layout = null;}
<!DOCTYPE html>
<html lang="tr">
@await Component.InvokeAsync("WriterPanelHeadSection")
<body>
    <div class="container-scroller">
        <!-- partial:../../partials/_navbar.html -->
        @await Component.InvokeAsync("WriterPanelNavbarSection")
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:../../partials/_sidebar.html -->
            @await Component.InvokeAsync("WriterPanelSidebarSection")

            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:../../partials/_footer.html -->
                @await Component.InvokeAsync("WriterPanelFooterSection")
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    @await Component.InvokeAsync("WriterPanelScriptsSection") 
</body>
</html>