@model List<About>
@{
    ViewData["Title"] = "Hakkımızda";
}
<section class="main-content-w3layouts-agileits">
    <div class="container">
        <h3 class="tittle">Hakkımızda</h3>
        <div class="row inner-sec">
            <div class="col-lg-8 left-blog-info-w3layouts-agileits text-left">
                <div class="blog-grid-top">
                    @foreach (About item in Model)
                    {
                        <div class="blog_info_left_grid">
                            <a href="single.html">
                                <img src="@item.ImageUrl1" class="img-fluid" alt="">
                            </a>
                        </div>
                        <h3>
                            Biz Kimiz?
                        </h3>
                        <p>
                            @item.Details1
                        </p>
                    }
                </div>
            </div>
            <aside class="col-lg-4 agileits-w3ls-right-blog-con text-right">
                <div class="right-blog-info text-left">
                    <div class="tech-btm widget_social">
                        <h4>Bize Ulaşın</h4>
                        <ul>
                            <li>
                                <a class="twitter" href="#">
                                    <i class="fab fa-twitter"></i>
                                    <span class="count">317K</span> Twitter
                                </a>
                            </li>
                            <li>
                                <a class="facebook" href="#">
                                    <i class="fab fa-facebook-f"></i>
                                    <span class="count">218k</span> Facebook
                                </a>
                            </li>
                            <li>
                                <a class="dribble" href="#">
                                    <i class="fab fa-instagram"></i>
                                    <span class="count">215k</span> Instagram
                                </a>
                            </li>
                            <li>
                                <a class="pin" href="#">
                                    <i class="fab fa-linkedin"></i>
                                    <span class="count">190k</span> Linkedin
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </aside>
        </div>
    </div>
</section>